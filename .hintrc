{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off",
        "aria-input-field-name": "off"
      }
    ]
  }
}